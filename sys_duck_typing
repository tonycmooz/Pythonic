import sys

while True:
  # print module presents output on the shell or screen 
  print "Let us run an iteration ..."
  # Here we use duck typing to receive inputs
  try:
    # reading from sys.stdin
    number = raw_input("Enter a number: ")
  except EOFError:
    print "\nBYE"
    break
  else:
    number = int(number)
    if number == 0:
      print >> sys.stderr, "0 has no inverse"
    else:
      print "inverse of %d is %f" % (number, 1.0/number)
